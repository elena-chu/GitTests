<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:converters="clr-namespace:Ws.Extensions.UI.Wpf.Converters">
  <!--Converters-->
  <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
  <converters:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter" />
  <converters:GridLengthMultiConverter x:Key="GridLengthMultiConverter" />
  <converters:InvertedBooleanConverter x:Key="InvertedBooleanConverter" />
  <converters:ConverterProxy x:Key="converterProxy" />
  <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
  <converters:PostfixFormatConverter x:Key="PostfixFormatConverter" />
  <converters:PrefixFormatConverter x:Key="PrefixFormatConverter" />
  <converters:MathConverter x:Key="MathConverter" />

  <converters:ComparisonConverter x:Key="ComparisonConverter" />
  <converters:ComparisonConverter x:Key="ComparisionToVisibilityConverter" TrueValue="Visible" FalseValue="Collapsed" />
  <converters:ComparisonConverter x:Key="ComparisionToBooleanConverter" TrueValue="True" FalseValue="False" />

</ResourceDictionary>