<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:sys="clr-namespace:System;assembly=mscorlib">

  <sys:Boolean x:Key="XBoolean.True">True</sys:Boolean>
  <sys:Boolean x:Key="XBoolean.False">False</sys:Boolean>

  <sys:String x:Key="XString.DegreesCelcius">&#176;C</sys:String>
  <sys:String x:Key="XString.DegreesFahrenheit">&#176;F</sys:String>
  <sys:String x:Key="XString.Joules">Joules</sys:String>
  <sys:String x:Key="XString.Kilojoules">kJ</sys:String>
  <sys:String x:Key="XString.Watt">Watt</sys:String>
  <sys:String x:Key="XString.Sec">sec</sys:String>
  
</ResourceDictionary>