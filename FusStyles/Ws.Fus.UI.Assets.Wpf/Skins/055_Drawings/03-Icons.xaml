<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:sys="clr-namespace:System;assembly=mscorlib">

  <sys:Double x:Key="XDouble.IconCanvasSize">50</sys:Double>
  
  <SolidColorBrush x:Key="XBrush.Icon.Alpha" Color="{StaticResource XColor.Application.Foreground}"/>
  <SolidColorBrush x:Key="XBrush.Icon.Beta" Color="{StaticResource XColor.Button.IconButton.Beta}"/>
  <SolidColorBrush x:Key="XBrush.Icon.Disabled" Color="{StaticResource XColor.Control.Foreground.Disabled}"/>

  <Style x:Key="XStyle.Canvas.Icon" TargetType="{x:Type Canvas}">
    <Setter Property="Height" Value="{StaticResource XDouble.IconCanvasSize}"/>
    <Setter Property="Width" Value="{StaticResource XDouble.IconCanvasSize}"/>
  </Style>
  
  <Style x:Key="XStyle.Shape.Icon.Line" TargetType="{x:Type Shape}">
    <Setter Property="StrokeThickness" Value="3"/>
    <Setter Property="StrokeLineJoin" Value="Round"/>
    <Setter Property="StrokeMiterLimit" Value="4"/>
    <Style.Triggers>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Stroke" Value="{StaticResource XBrush.Icon.Disabled}"/>
      </Trigger>
    </Style.Triggers>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.Area" TargetType="{x:Type Shape}">
    <Style.Triggers>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Fill" Value="{StaticResource XBrush.Icon.Disabled}"/>
      </Trigger>
    </Style.Triggers>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.LineArea" TargetType="{x:Type Shape}" BasedOn="{StaticResource XStyle.Shape.Icon.Line}">
    <Style.Triggers>
      <Trigger Property="IsEnabled" Value="False">
        <Setter Property="Fill" Value="{StaticResource XBrush.Icon.Disabled}"/>
      </Trigger>
    </Style.Triggers>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.LineAlpha" TargetType="{x:Type Shape}" BasedOn="{StaticResource XStyle.Shape.Icon.Line}">
    <Setter Property="Stroke" Value="{StaticResource XBrush.Icon.Alpha}"/>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.LineBeta" TargetType="{x:Type Shape}" BasedOn="{StaticResource XStyle.Shape.Icon.Line}">
    <Setter Property="Stroke" Value="{StaticResource XBrush.Icon.Beta}"/>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.AreaAlpha" TargetType="{x:Type Shape}" BasedOn="{StaticResource XStyle.Shape.Icon.Area}">
    <Setter Property="Fill" Value="{StaticResource XBrush.Icon.Alpha}"/>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.AreaBeta" TargetType="{x:Type Shape}" BasedOn="{StaticResource XStyle.Shape.Icon.Area}">
    <Setter Property="Fill" Value="{StaticResource XBrush.Icon.Beta}"/>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.LineAreaAlpha" TargetType="{x:Type Shape}" BasedOn="{StaticResource XStyle.Shape.Icon.LineArea}">
    <Setter Property="Stroke" Value="{StaticResource XBrush.Icon.Alpha}"/>
    <Setter Property="Fill" Value="{StaticResource XBrush.Icon.Alpha}"/>
  </Style>

  <Style x:Key="XStyle.Shape.Icon.LineAreaBeta" TargetType="{x:Type Shape}" BasedOn="{StaticResource XStyle.Shape.Icon.LineBeta}">
    <Setter Property="Stroke" Value="{StaticResource XBrush.Icon.Beta}"/>
    <Setter Property="Fill" Value="{StaticResource XBrush.Icon.Beta}"/>
  </Style>

