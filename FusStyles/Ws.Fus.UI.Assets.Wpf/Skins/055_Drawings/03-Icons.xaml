  <Canvas x:Key="XCanvas.Redo" Style="{StaticResource XStyle.Canvas.Icon}">
    <Path Style="{StaticResource XStyle.Shape.Icon.LineAlpha}" Data="{StaticResource XGeometry.Undo}">
      <Path.RenderTransform>
        <TransformGroup>
          <ScaleTransform ScaleX="-1"/>
          <TranslateTransform X="{StaticResource XDouble.IconCanvasSize}"/>
        </TransformGroup>
      </Path.RenderTransform>
    </Path>
  </Canvas>

