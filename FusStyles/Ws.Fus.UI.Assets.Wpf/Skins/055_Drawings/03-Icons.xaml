      Style="{StaticResource XStyle.Shape.Icon.LineAlpha}"
      Width="{Binding Height, RelativeSource={RelativeSource AncestorType={x:Type Canvas}}}"
      Height="{Binding Height, RelativeSource={RelativeSource AncestorType={x:Type Canvas}}}"/>
    <Path Style="{StaticResource XStyle.Shape.Icon.AreaAlpha}" Data="{StaticResource XGeometry.ExclamationMark}"/>
  </Canvas>

  <Canvas x:Key="XCanvas.ActionRequired" Style="{StaticResource XStyle.Canvas.Icon}">
    <Ellipse
      Style="{StaticResource XStyle.Shape.Icon.LineAlpha}"
      StrokeDashArray="3,2"
      StrokeDashCap="Round"
      Width="{Binding Height, RelativeSource={RelativeSource AncestorType={x:Type Canvas}}}"
      Height="{Binding Height, RelativeSource={RelativeSource AncestorType={x:Type Canvas}}}"/>
    <Path Style="{StaticResource XStyle.Shape.Icon.LineAlpha}" Data="{StaticResource XGeometry.Checkmark}"/>
  </Canvas>

  <Canvas x:Key="XCanvas.Done" Style="{StaticResource XStyle.Canvas.Icon}">
